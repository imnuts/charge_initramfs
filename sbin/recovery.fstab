# mount     fstype  device                  [device2]   [fstype2]   [fsoptions]         [fsoptions2]

/boot       bml     boot
/cache      rfs     /dev/block/mmcblk0p3
/data       rfs     /dev/block/mmcblk0p1
/datadata   rfs     /dev/block/stl11
/recovery   bml     recovery
/sdcard     vfat    /dev/block/mmcblk1p1
/system     rfs     /dev/block/stl10
